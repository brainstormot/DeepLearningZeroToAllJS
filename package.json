{
  "name": "deeplearningzerotoalljs",
  "version": "1.0.0",
  "description": "Tensorflow.js implementation of DeepLearningZeroToAll",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "build:ts": "tsc",
    "build:html": "cpy '**/*.html' '../dist/' --cwd=src --parents --rename=index.html",
    "build:css": "cpy '**/*.css' '../dist/' --cwd=src --parents",
    "build": "npm run build:ts && npm run build:html && npm run build:css",
    "server": "http-server ./dist -o",
    "clean:dist": "rm -rf dist",
    "clean": "npm run clean:dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brainstormot/DeepLearningZeroToAllJS.git"
  },
  "keywords": [
    "tensorflow.js",
    "javascript",
    "typescript"
  ],
  "author": "Hyuck Kang",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/brainstormot/DeepLearningZeroToAllJS/issues"
  },
  "homepage": "https://github.com/brainstormot/DeepLearningZeroToAllJS#readme",
  "dependencies": {
    "@tensorflow/tfjs": "^0.12.0",
    "d3": "^5.5.0"
  },
  "devDependencies": {
    "cpy-cli": "^2.0.0",
    "http-server": "^0.11.1",
    "typescript": "^2.9.2"
  }
}
